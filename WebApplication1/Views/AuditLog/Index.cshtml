@model IEnumerable<WebApplication1.Entities.AuditLog>

<h2>Dashboard</h2>

<table class="table">
    <thead>
        <tr>
            <th>User ID</th>
            <th>UserName</th>
            <th>Action</th>
            <th>Date / Time</th>
            <th>IP Address</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.UserId</td>
                <td>@log.UserName</td>
                <td>@log.Action</td>
                <td>@log.Timestamp.ToLocalTime().ToString("dd.MM.yyyy  HH:mm:ss")</td>
                <td>@log.IpAddress</td>
                <td>@log.Details</td>
            </tr>
        }
    </tbody>
</table>
